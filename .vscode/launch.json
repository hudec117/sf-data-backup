{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch SfJwtGenerator",
            "type": "coreclr",
            "request": "launch",
            "program": "${workspaceFolder}/src/SfJwtGenerator/bin/Debug/netcoreapp3.1/SfJwtGenerator.dll",
            "cwd": "${workspaceFolder}/src/SfJwtGenerator",
            "stopAtEntry": false,
            "console": "internalConsole"
        },
        {
            "name": "Attach to .NET Functions",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:azureFunctions.pickProcess}"
        }
    ]
}